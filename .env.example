# === LLM 설정 ===
LLM_PROVIDER=openai
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_MODEL=gemini-pro
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_API_KEY=your_ollama_api_key_here
OLLAMA_MODEL=llama2
DEEPINFRA_API_KEY=your_deepinfra_api_key_here
DEEPINFRA_MODEL=meta-llama/Llama-2-70b-chat-hf
DEEPINFRA_BASE_URL=https://api.deepinfra.com/v1/openai
LLM_TEMPERATURE=0.7
LLM_MAX_TOKENS=1000
LLM_TIMEOUT=30
LLM_MAX_RETRIES=3

# === Redis 설정 ===
REDIS_SENTINEL_HOSTS=localhost:26379
REDIS_SENTINEL_SERVICE_NAME=mymaster
REDIS_PASSWORD=your_redis_password
REDIS_DB=0
REDIS_SOCKET_TIMEOUT=30
REDIS_SOCKET_CONNECT_TIMEOUT=30
REDIS_MAX_CONNECTIONS=50

# === OpenSearch 설정 ===
OPENSEARCH_HOSTS=localhost:9200
OPENSEARCH_USER=admin
OPENSEARCH_PASSWORD=admin
OPENSEARCH_USE_SSL=false
OPENSEARCH_VERIFY_CERTS=false
OPENSEARCH_SSL_ASSERT_HOSTNAME=false
OPENSEARCH_SSL_SHOW_WARN=false
OPENSEARCH_TIMEOUT=30
OPENSEARCH_MAX_RETRIES=3
OPENSEARCH_RETRY_ON_TIMEOUT=true

# === MariaDB 설정 ===
MARIADB_HOST=localhost
MARIADB_PORT=3306
MARIADB_USER=root
MARIADB_PASSWORD=your_mariadb_password
MARIADB_DATABASE=investment_analyst
MARIADB_CHARSET=utf8mb4
MARIADB_CONNECT_TIMEOUT=10
MARIADB_READ_TIMEOUT=30
MARIADB_WRITE_TIMEOUT=30

# === 초기화 플래그 ===
DB_INITIALIZED=false

# === Autogen 설정 ===
AUTOGEN_ENABLED=true
AUTOGEN_FINANCE_GROUP=finance_analysts
AUTOGEN_TRAVEL_GROUP=travel_experts
AUTOGEN_BANKING_GROUP=banking_specialists
AUTOGEN_MAX_ROUND=10
AUTOGEN_TIMEOUT=300
AUTOGEN_CODE_EXECUTION=false
AUTOGEN_HUMAN_FEEDBACK=false
AUTOGEN_PARALLEL=true
AUTOGEN_CACHE=true

# === Langfuse 설정 ===
LANGFUSE_ENABLED=false
LANGFUSE_HOST=https://cloud.langfuse.com
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key
LANGFUSE_SECRET_KEY=your_langfuse_secret_key
LANGFUSE_DEBUG=false
LANGFUSE_TRACE_LLM=true
LANGFUSE_TRACE_AGENTS=true
LANGFUSE_TRACE_AUTOGEN=true
LANGFUSE_FLUSH_AT=15
LANGFUSE_FLUSH_INTERVAL=0.5
LANGFUSE_TIMEOUT=20

# === Phoenix 설정 ===
PHOENIX_ENABLED=false
PHOENIX_ENDPOINT=http://localhost:6006
PHOENIX_GRPC_ENDPOINT=http://localhost:4317
PHOENIX_TRACE_LLM=true
PHOENIX_TRACE_EMBEDDINGS=true
PHOENIX_TRACE_RETRIEVALS=true
PHOENIX_COLLECT_METRICS=true
PHOENIX_SAMPLE_RATE=1.0

# === 검색 API 설정 ===
GOOGLE_SEARCH_API_KEY=your_google_search_api_key
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret
DAUM_APP_KEY=your_daum_app_key

# === 금융 API 설정 ===
BOK_API_KEY=your_bok_api_key
FSS_API_KEY=your_fss_api_key
KOREAEXIM_API_KEY=your_koreaexim_api_key
KFTC_API_KEY=your_kftc_api_key
KFTC_CLIENT_ID=your_kftc_client_id
KFTC_CLIENT_SECRET=your_kftc_client_secret

# === 부동산 API 설정 ===
MOLIT_API_KEY=your_molit_api_key
KOREA_LAND_API_KEY=your_korea_land_api_key
KB_REAL_ESTATE_API_KEY=your_kb_real_estate_api_key

# === 서버 설정 ===
API_HOST=0.0.0.0
API_PORT=2400
API_RELOAD=true
API_LOG_LEVEL=info

# === 데이터베이스 설정 ===
DB_CONNECTION_POOL_SIZE=10
TESTING_DATABASE_URL=sqlite:///test_investment_analyst.db

# === 캐시 설정 ===
CACHE_TTL_SECONDS=3600
CACHE_HIT_THRESHOLD=0.8
CACHE_EVICTION_POLICY=lru
PRELOAD_CACHE_ENABLED=true

# === 쿼리 설정 ===
QUERY_TIMEOUT_SECONDS=30

# === 개발 설정 ===
DEBUG_MODE=false
LOG_LEVEL=INFO

# === 보안 설정 ===
SECRET_KEY=your_secret_key_here_change_in_production
ACCESS_TOKEN_EXPIRE_MINUTES=30